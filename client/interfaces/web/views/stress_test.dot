<html>

	<head>
		<title>CherryPlum Stress Test</title>
		<meta charset="utf-8">

		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/latest/css/bootstrap.min.css">
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
		<link rel="stylesheet" type="text/css" href="/res/web/main.css" />
		<meta name="viewport" content="width=device-width, initial-scale=1">
	</head>




	<body>


		<div class="container">
			<div class="row">
				<div class="col-xs-12">

					<h1>Stress test</h1>


					<hr><hr>

					<div class="form-group row">
						<label for="ajax-requets-amount" class="col-xs-6 col-form-label">How Many Ajax Requets:</label>
						<div class="col-xs-2">
							<input class="form-control" type="number" value="10" id="ajax-requets-amount">
						</div>

					</div>


					<div class="form-group row">
						<label for="ajax-requets-interval" class="col-xs-6 col-form-label">Consecutive Ajax Requets interval:</label>
						<div class="col-xs-2">
							<input class="form-control" type="number" value="100" id="ajax-requets-interval">
						</div>
					</div>


					<div class="form-group row">
						<label for="documents-inserted-per-100-requests" class="col-xs-6 col-form-label">Documents inserted per 100 requests:</label>
						<div class="col-xs-2">
							<input class="form-control" type="number" value="100" id="documents-inserted-per-100-requests">
						</div>
					</div>



					<div class="form-group row">
						<label for="documents-retrieved-per-100-requests" class="col-xs-6 col-form-label">Documents retrieved per 100 requests:</label>
						<div class="col-xs-2">
							<input class="form-control" type="number" value="100" id="documents-retrieved-per-100-requests">
						</div>
					</div>


					<!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
					<button type="button" class="btn btn-primary btn-lg col-xs-3" id="start_stop_test">Start new Test</button>



					<br><br><br>
					<br><br><br>


					<div class="table-responsive">
						<table class="table">
							
							<tr class="success">
								<th>Requests sent so far:</th>
								<td id="requests-sent-screen">321</td>
							</tr>

							<tr class="active">
								<th>Requests received so far:</th>
								<td id="requests-recieved-screen">432</td>
							</tr>

							
							<tr class="info">
								<th>Documents inserted so far:</th>
								<td id="documents-inserted-screen">76</td>
							</tr>

							<tr class="warning">
								<th>Documents Retrieved so far:</th>
								<td id="documnets-recieved-screen">54</td>
							</tr>

							
							
							<tr class="success">
								<th>Average time per request:</th>
								<td id="request-average-time-screen">324</td>
							</tr>

							
							<tr class="active">
								<th>Average time per document inserted:</th>
								<td id="insert-average-time-screen">54</td>
							</tr>

							
							<tr class="info">
								<th>Average time per document retrieved:</th>
								<td id="retrieve-average-time-screen">54</td>
							</tr>

						</table>
					</div>

					<br><br><br>

					<div class="row">
						<div class="col-xs-10">
						
							<!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
							<button type="button" id="check_number_of_documents" class="btn btn-warning btn-lg">Check number of documents in the test collection</button>
						
						</div>

						<div class="col-xs-2 warning" id="number_of_docs_screen">
							432
						</div>
					</div>

					
					<br><br><br>
					<div class="row">

						<div class="col-xs-12">
							
							<!-- Provides extra visual weight and identifies the primary action in a set of buttons -->
							<button type="button" id="cleanup_test_collection" class="btn btn-danger btn-lg">Cleanup test collection</button>
						</div>

					</div>


				
				
				</div>
			</div>
		</div>
		



		<script   src="https://code.jquery.com/jquery-2.2.4.js"   integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI="   crossorigin="anonymous"></script>

		
		<script   src="/res/web/main.js"   ></script>

	</body>






</html>
